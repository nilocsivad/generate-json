<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- com.iam_vip.generate_json.zz.backend.login.GJLogin -->
<mapper namespace="com.iam_vip.generate_json.dal.backend.IDALLogin">

	<sql id="TABLE_NAME"> tbl_login </sql>



	<parameterMap type="com.iam_vip.generate_json.zz.backend.login.GJLogin" id="GJLogin"></parameterMap>



	<resultMap type="com.iam_vip.generate_json.zz.backend.login.GJLogin" id="GJLoginMap" autoMapping="true"></resultMap>



	<insert id="insert" parameterMap="GJLogin">
		INSERT INTO
		<include refid="TABLE_NAME" />
		(
		uid,
		ln,
		pwd,
		createDT,
		type,
		status
		)
		VALUES(
		#{uid},
		#{ln},
		#{pwd},
		#{createDT},
		#{type},
		#{status}
		)
	</insert>



	<select id="queryCountEq" resultType="java.lang.Integer">
		SELECT COUNT(*) FROM
		<include refid="TABLE_NAME" />
		<trim prefix="WHERE" prefixOverrides="AND">
			<if test="uid != null and uid != ''"> AND uid = #{uid} </if>
			<if test="ln != null and ln != ''"> AND ln = #{ln} </if>
			<if test="pwd != null and pwd != ''"> AND pwd = #{pwd} </if>
			<if test="createDT != null and createDT != ''"> AND createDT = #{createDT} </if>
			<if test="type != null and type != ''"> AND type = #{type} </if>
			<if test="status != null and status != ''"> AND status = #{status} </if>
		</trim>
	</select>

</mapper>